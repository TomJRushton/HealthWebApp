{
  "name": "ehealth-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest --coverage=false"
  },
  "dependencies": {
    "@renatodeleao/vue-sticky-directive": "^0.3.0",
    "bootstrap-vue": "^2.3.0",
    "chart.js": "^2.9.3",
    "core-js": "^3.4.4",
    "firebase": "^7.7.0",
    "firebase-mock": "^2.3.2",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.5.0",
    "vue-lottie": "^0.2.1",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.2.3",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sonarqube-scanner": "^2.5.0",
    "jest": "^25.1.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "description": "## Project setup ``` npm install ```",
  "main": "babel.config.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/cssd1/ehealth-web-app.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/cssd1/ehealth-web-app/issues"
  },
  "homepage": "https://gitlab.com/cssd1/ehealth-web-app#readme"
}
